<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="productImageMapper">
	<insert id="insert" parameterType="ProductImageDTO">
		insert into ps_img(num, name, fname, oname) values(ps_img_seq.nextval,#{name},#{fname},#{oname})
	</insert>
	<select id="selectImages" parameterType="java.lang.String" resultType="ProductImageDTO">
		select * from ps_img where name = #{name}
	</select>
  </mapper>